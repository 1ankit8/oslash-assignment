{
  "short_name": "Scheduler",
  "name": "Google Event Creator",
  "manifest_version": 2,
  "author": "Ankit Kumar",
  "version": "0.0.1",
  "description": "Create Google Event with Google Meet Link",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo16.png",
      "32": "logo32.png",
      "24": "logo24.png",
      "64": "logo64.png",
      "192": "logo192.png",
      "512": "logo512.png"
    }
  },
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "24": "logo24.png",
    "64": "logo64.png",
    "192": "logo192.png",
    "512": "logo512.png"
  }
}